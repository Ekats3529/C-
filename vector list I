#include <iostream>
#include <fstream>
#include <cmath>
#include <vector>
#include <list>
#include <string>
#include <algorithm>
using namespace std;

bool Correct(int n) {
    if (n < 2) {
        cout << "Error: the count of numbers must be more than 2. Try again";
    }
    return (n < 2);

}


int main()
{
    // A
    int n, num;
    vector<int> v;
    cin >> n;
    if (!Correct(n)) { cin >> n; }
    for (int i = 0; i < n; i++) {
        cin >> num;
        v.push_back(num);
    }
    int key = v[v.size() - 1];
    v[0] = -key;
    v[1] = -key;
    for (int i = 0; i < v.size(); i++) { cout << v[i] << " "; }
    cout << endl;
    //B
    int n, num;
    list<int> l;
    cin >> n;
    if (!Correct(n)) { cin >> n; }
    for (int i = 0; i < n; i++) {
        cin >> num;
        l.push_back(num);
    }
    if (l.front() < l.back()) {
        l.pop_back();
    }
    else {
        l.pop_front();
    }
    for (list <int> ::iterator i = l.begin(); i != l.end(); i++) {
        cout << (*i) << " ";
    }

    return 0;
}
