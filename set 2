#include <iostream>
#include <fstream>
#include <cmath>
#include <vector>
#include <list>
#include <set>
#include <string>
#include <algorithm>
using namespace std;


int main()
{
    set<char> lower;
    set<char> upper;
    int n;
    string s;
    cin >> n;
    for (int t = 0; t < n + 1; t++) {
        getline(cin, s);
        for (int i = 0; i < s.length(); i++) {
            if (isupper(s[i])) { upper.insert(s[i]); }
            if (islower(s[i])) { lower.insert(s[i]); }
        }
    }
    set<char> ::iterator it = lower.begin();
    set<char> ::iterator it = upper.begin();
    while (it != lower.end()) {
        cout << *it << endl;
        it++;
    }

    
    return 0;
}
